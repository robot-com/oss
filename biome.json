{
    "$schema": "https://biomejs.dev/schemas/2.1.4/schema.json",
    "formatter": {
        "enabled": true,
        "indentWidth": 4,
        "indentStyle": "space",
        "lineEnding": "lf"
    },
    "javascript": {
        "formatter": {
            "quoteStyle": "single",
            "jsxQuoteStyle": "single",
            "trailingCommas": "es5",
            "semicolons": "asNeeded",
            "quoteProperties": "asNeeded"
        }
    },
    "linter": {
        "enabled": true,
        "rules": {
            "style": {
                "noNonNullAssertion": "off"
            }
        }
    },
    "assist": {
        "enabled": true
    }
}
